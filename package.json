  "scripts": {
    "generate:api": "rimraf generated && mkdir generated && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pn-logextractor-be/develop/docs/openapi/swagger.yaml --no-strict --out-dir ./generated --request-types --response-decoders --client",
    "generate:api-paper-channel": "openapi-generator-cli generate -g typescript-axios -i https://raw.githubusercontent.com/pagopa/pn-paper-channel/develop/docs/openapi/pn-paper-channel-v1.yaml -o ./src/api/paperChannel --additional-properties=supportsES6=true",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --watchAll=false --ci --coverage --testResultsProcessor=jest-sonar-reporter",
    "eject": "react-scripts eject",
    "sonar": "node sonarqube-scanner.js",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "format": "prettier \"./**/*.{ts,tsx}\" --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@pagopa/openapi-codegen-ts": "^12.1.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.0.0",
    "axios-mock-adapter": "^1.21.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "jest": "^28.1.1",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "sonarqube-scanner": "^3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/components/**",
      "**/src/pages/**",
      "!**/src/components/formFields/**"
    ]
  },
  "resolutions": {
    "**/react-scripts/react-dev-utils/recursive-readdir/minimatch": "^3.0.5",
    "**/react-scripts/@svgr/webpack/@svgr/plugin-svgo/svgo/css-select/nth-check": "^2.0.1",
    "**/terser": "^5.14.2",
    "**/file-loader/loader-utils": "^2.0.4",
    "**/react-scripts/react-dev-utils/loader-utils": "^3.2.1"
  }
